{
    "family": "kusorep-task-definition",
    "taskRoleArn": "arn:aws:iam::719378226820:role/ECSTaskRole",
    "executionRoleArn": "arn:aws:iam::719378226820:role/ECSTaskRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "719378226820.dkr.ecr.us-east-2.amazonaws.com/kusorep-api/nginx:latest",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "readonlyRootFilesystem": false,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-hands-on",
                    "awslogs-region": "us-east-2",
                    "awslogs-datetime-format": "%Y-%m-%d %H:%M:%S",
                    "awslogs-stream-prefix": "laravel"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}